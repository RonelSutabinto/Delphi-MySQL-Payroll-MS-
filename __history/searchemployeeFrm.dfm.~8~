  object empQ: TMyQuery
    Connection = PayrollData.MyConnection1
    SQL.Strings = (
      'Select'
      #9' idemployee,'
      #9' empnumber,'#9' '
      #9' concat(firstname,'#39' '#39',middleinitial,'#39', '#39',lastname) as name,'
      #9' departmentname as department,'
      #9' case employeestatus'
      #9'   when '#39'C'#39' then '#39'Casual'#39
      #9'   when '#39'R'#39' then '#39'Regular'#39
      '           when '#39'B'#39' then '#39'Board'#39
      #9'   ELSE '#39#39
      #9' END as empstatus '
      ' from employee'
      
        ' where concat(firstname,'#39' '#39',middleinitial,'#39', '#39',lastname) like :n' +
        'ame')
    Left = 336
    Top = 46
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'name'
        Value = nil
      end>
    object empQidemployee: TLongWordField
      FieldName = 'idemployee'
    end
    object empQempnumber: TStringField
      FieldName = 'empnumber'
      Size = 15
    end
    object empQname: TStringField
      FieldName = 'name'
      Size = 68
    end
    object empQdepartment: TStringField
      FieldName = 'department'
      Size = 40
    end
    object empQempstatus: TStringField
