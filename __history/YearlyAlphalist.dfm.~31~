  object SearchPayroll: TMyQuery
    Connection = PayrollData.MyConnection1
    SQL.Strings = (
      
        'select Description from payrollmaster where description like :de' +
        'sc and year(paymonth) = year(:df) '
      'group by description')
    Left = 32
    Top = 368
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DESC'
      end
      item
        DataType = ftUnknown
        Name = 'df'
      end>
