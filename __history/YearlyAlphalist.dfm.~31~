  end
  object insertAlphanontaxable: TMyQuery
    Connection = PayrollData.MyConnection1
    SQL.Strings = (
      'Insert into alphalistbenefits'
      '('
      ' idemployee,'
      ' lastname,'
      ' firstname,'
      ' amount,'
      ' description,'
      ' daypresent,'
      ' basicpay,'
      ' paymonth'
      ')'
      ''
      '('
      ' select'
      '  e.idemployee,'
      '  e.lastname,'
      '  e.firstname,'
      '  pm.netpay,'
      '  pm.description,'
      '  pm.daypresent,'
      
        '  if(ifnull(pm.daypresent,0) = 0,pm.basicpay,(ifnull(pm.otherpay' +
        ',0)/ 22) * daypresent),'
      '  pm.paymonth'
      
        ' from payrollmaster pm inner join employee e on e.idemployee = p' +
        'm.idemployee'
      ' where year(paymonth) = year(:df) and description = :desc'
      ')')
    Left = 624
    Top = 24
    ParamData = <
      item
        DataType = ftString
        Name = 'df'
        Value = '2011-01-01'
      end
      item
        DataType = ftString
        Name = 'DESC'
        Value = 'regular payroll'
      end>
    object insertAlphanontaxableDescription: TStringField
      FieldName = 'Description'
    end
  end
