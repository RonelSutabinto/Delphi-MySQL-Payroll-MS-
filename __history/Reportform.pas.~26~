    procedure QRBand72BeforePrint(Sender: TQRCustomBand;
      var PrintBand: Boolean);
    procedure QRBand28BeforePrint(Sender: TQRCustomBand;
      var PrintBand: Boolean);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Reports: TReports;

implementation

uses FlexMenu, Data;

{$R *.dfm}

procedure TReports.QRBand28BeforePrint(Sender: TQRCustomBand;
  var PrintBand: Boolean);
begin
  qrlabel206.Caption := '          We hereby certify and acknowledge the receipt of ' +
                              'our ' + PrintPayrollMasterdescription.Text + FPPayrollMainMenuForm.nxedit3.text;
end;

procedure TReports.QRBand72BeforePrint(Sender: TQRCustomBand;
  var PrintBand: Boolean);
begin
  qrlOtherPayTitle.Caption := '          We hereby certify and acknowledge the receipt of ' +
                              'our ' + PrintPayrollMasterdescription.Text + FPPayrollMainMenuForm.nxedit3.text;
end;

end.
