  object paymonthDetailsQ: TMyQuery
    Connection = PayrollData.MyConnection1
    SQL.Strings = (
      'Select '
      '  f.*,'
      
        '  Concat('#39'Payroll Month: '#39',DATE_FORMAT(f.paymonth, "%M, %Y")) da' +
        'teF'
      'from ('
      'Select p.*,e.deptcode,e.pagibigno,d.departmentname'
      'from zanecopayroll.pmdeduction p '
      'left join zanecopayroll.employee e on e.empnumber = p.empnumber'
      
        'left join zanecopayroll.department d on d.departmentcode = e.dep' +
        'tcode'
      'where p.description = :descrp and '
      '      p.paymonth = :paymonth  and'
      '      p.paydesc = :paydesc order by p.name '
      ') f'
      'order by f.deptcode,f.name asc')
    Active = True
    Left = 688
    Top = 56
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'descrp'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'paymonth'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'paydesc'
        Value = nil
      end>
