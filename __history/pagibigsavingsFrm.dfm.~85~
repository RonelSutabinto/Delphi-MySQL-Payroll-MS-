  object savingQ: TMyQuery
    Connection = PayrollData.MyConnection1
    SQL.Strings = (
      'Select f.*'
      'from'
      '(SELECT '
      ' p.idemployee,'
      ' p.empnumber,'
      ' p.amount,'
      
        ' concat(e.firstname,'#39' '#39',e.middleinitial,'#39', '#39',e.lastname) as name' +
        ','
      ' e.departmentname,'
      ' case e.employeestatus'
      '   when '#39'C'#39' then '#39'Casual'#39
      '   when '#39'R'#39' then '#39'Regular'#39
      '   when '#39'B'#39' then '#39'Board'#39
      '   ELSE '#39#39
      ' END as empstatus'
      ''
      'FROM pagibigsavingsdebt p'
      'left join employee e on e.idemployee = p.idemployee'
      ') f'
      'where f.name like :name')
    Left = 200
    Top = 264
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'name'
        Value = nil
      end>
    object savingQidemployee: TIntegerField
      FieldName = 'idemployee'
    end
    object savingQempnumber: TStringField
      FieldName = 'empnumber'
      Size = 15
    end
    object savingQamount: TFloatField
      FieldName = 'amount'
      currency = True
    end
    object savingQname: TStringField
      FieldName = 'name'
      Size = 68
    end
    object savingQdepartmentname: TStringField
      FieldName = 'departmentname'
      Size = 40
    end
    object savingQempstatus: TStringField
      FieldName = 'empstatus'
      Size = 7
    end
  end
