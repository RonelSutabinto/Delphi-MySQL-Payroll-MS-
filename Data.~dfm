  object NR3001dk: TMyQuery
    Connection = MyConnection1
    SQL.Strings = (
      'select e.lastname,   '
      '       e.firstname,'
      '       e.middlename,'
      '       e.sssnumber,'
      
        '       ifnull(p.employeecontribution+p.employercontribution,0) S' +
        'SSContribution,'
      '       ifnull(p.employeecompensation,0) Compensation'
      'from pmdeduction p'
      '  inner join employee e on (e.idemployee = p.idemployee)'
